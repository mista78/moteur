{
    "name": "carmf/ij-calculator",
    "description": "CARMF IJ Calculator with Slim Framework",
    "type": "project",
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7.0",
        "monolog/monolog": "^3.5",
        "cakephp/cakephp": "^5.0",
        "cakephp/migrations": "^4.0"
    },
    "require-dev": {
        "cakephp/bake": "^3.0",
        "cakephp/debug_kit": "^5.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
